{
    "name": "frontend_nextjs",
    "version": "2.0.0-beta-1",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3000",
        "build": "next build",
        "build:local": "IGNORE_SENTRY=1 next build",
        "start": "next start -p 3000",
        "export": "next export",
        "storybook": "start-storybook -p 3001 -s ./public",
        "build-storybook": "npx build-storybook -s public",
        "fixcode": "npx prettier \"**/*(*.css|*.ts|*.json|*.md|*.scss)\" --write",
        "new": "node cli/bin.js new",
        "new:container": "node cli/bin.js new --container",
        "test": "jest --watch",
        "test:ci": "npx jest --ci --runInBand --reporters=default --reporters=jest-junit",
        "lint": "next lint"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.2",
        "@heroicons/react": "^2.0.11",
        "@types/react": "^18.0.15",
        "classnames": "^2.3.1",
        "es-cookie": "^1.4.0",
        "i18next": "^21.8.5",
        "lodash": "^4.17.21",
        "next": "12.3.0",
        "prop-types": "^15.8.1",
        "react": "18.2.0",
        "react-cookie-consent": "^8.0.1",
        "react-dom": "18.2.0",
        "react-ga": "^3.3.1",
        "react-player": "^2.11.0",
        "rehype-parse": "^8.0.4",
        "rehype-stringify": "^9.0.3",
        "unified": "^10.1.2",
        "unist-util-visit": "^4.1.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --pattern 'frontend/**' && jest --onlyChanged",
            "pre-push": ""
        }
    },
    "devDependencies": {
        "@babel/core": "^7.17.10",
        "@storybook/addon-a11y": "^6.4.22",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/addon-viewport": "^6.4.22",
        "@storybook/addons": "^6.4.22",
        "@storybook/builder-webpack5": "^6.4.22",
        "@storybook/manager-webpack5": "^6.4.22",
        "@storybook/react": "^6.4.22",
        "@svgr/webpack": "^6.2.1",
        "@swc/core": "^1.2.196",
        "@swc/jest": "^0.2.21",
        "@tailwindcss/line-clamp": "^0.4.2",
        "@tailwindcss/typography": "^0.5.7",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^14.2.0",
        "@types/jest": "^29.2.0",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "autoprefixer": "^10.4.7",
        "babel-loader": "^8.2.5",
        "babel-plugin-transform-dynamic-import": "^2.1.0",
        "commander": "^9.4.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "eslint": "8.18.0",
        "eslint-config-next": "12.2.0",
        "husky": "^8.0.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.0.0",
        "jest-environment-jsdom": "^29.2.2",
        "jest-fetch-mock": "^3.0.3",
        "jest-junit": "^13.2.0",
        "jest-next-dynamic": "^1.0.1",
        "postcss": "^8.4.14",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "storybook-addon-next": "^1.6.9",
        "style-loader": "^3.3.1",
        "tailwindcss": "^3.1.8",
        "ts-jest": "^29.0.3",
        "tsconfig-paths-webpack-plugin": "^4.0.0"
    }
}
